type BencanaInCity {
  id: ID!
  name: String!
  city: City!
  predictions: [Prediction]
}

type BencanaInLocation {
  id: ID!
  name: String!
  location: Location!
  prediction = Prediction
}

type City {
  id: ID!
  name: String!
  locations:  [Location]
}

type Location {
  id: ID!
  city: City!
  lat_long: String!
  name: String!
}

type Prediction {
  id: ID!
  bencana: Bencana!
  location: Location!
  confidence: Float!
  time: DateTime!
  reason: String
}